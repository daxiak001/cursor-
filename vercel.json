{
    "version": 2,
    "builds": [
        {
            "src": "api/main.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/main.py"
        }
    ],
    "env": {
        "CLAUDE_API_KEY": "@claude_api_key"
    }
}

